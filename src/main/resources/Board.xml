<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
        version="3.0">

    <entity class="ru.mai.course.demo.Client.Board">
        <table name="board" schema="public" catalog="avia_ticekts"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="seatsNumber">
                <column name="seats_number" precision="-1"/>
            </basic>
            <basic name="destination">
                <column name="destination" nullable="false"/>
            </basic>
            <basic name="startPoint">
                <column name="start_point" nullable="false"/>
            </basic>
            <basic name="boardIndex">
                <column name="board_index" length="40"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
